include LICENSE
include README.md
include CONTRIBUTING.md

include setup.cfg
include pyproject.toml
include Makefile

graft ipyregulartable
graft ipyregulartable/extension
graft ipyregulartable/labextension
graft ipyregulartable/nbextension

# get rid of binder
prune binder

# Get rid of docs
prune docs

# Webapp files
graft js
prune js/build
prune js/dist
prune js/docs
prune js/lib
prune js/node_modules
prune js/yarn.lock

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints
